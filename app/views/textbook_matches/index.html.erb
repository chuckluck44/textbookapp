<p id="notice"><%= notice %></p>

<h1>Listing Textbook Matches</h1>

<table>
  <thead>
    <tr>
      <th>From user</th>
      <th>To user</th>
      <th>Textbook</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @textbook_matches.each do |textbook_match| %>
      <tr>
        <td><%= textbook_match.from_user_id %></td>
        <td><%= textbook_match.to_user_id %></td>
        <td><%= textbook_match.textbook_id %></td>
        <td><%= link_to 'Show', textbook_match %></td>
        <td><%= link_to 'Edit', edit_textbook_match_path(textbook_match) %></td>
        <td><%= link_to 'Destroy', textbook_match, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Textbook match', new_textbook_match_path %>
